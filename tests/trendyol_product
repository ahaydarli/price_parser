<script>
    window.addEventListener('load', function() {
        function getToken(){
            try {
                var authCookieName = 'COOKIE_TY.Entrance';
                var cookie = document.cookie.match(new RegExp('(^| )' + authCookieName + '=([^;]+)'))[2];
                var token = cookie.split('tx=')[1].split('&')[0];
                return token || null;
            } catch (e) {
                return null;
            }
        }

        function onLoad(){
            if(typeof window.WebPushManager !== "undefined"){
                window.WebPushManager(webPushConfiguration);
            }
        }

        var webPushConfiguration = {
            bundleSource: 'https://cdn.dsmcdn.com/mweb/production/client.7f1de7a56ca9e5783eaa.bundle.js',
            serviceworker: '/sw.js',
            publicKey: 'BGHagF3bwnkMxiY1IABk5zBNUW_BlVwaJ8Q2FrmQPb5ZG61Yf6uHVZpf6uhG7t7cIgkrN5w_o2qUY4eGZOGxdfc',
            subscriptionHost: 'https://api.trendyol.com/webpush',
            platformId: 1,
            storefrontId: 1
        };

        var userToken =  getToken();
        webPushConfiguration.user = userToken ? { token: userToken }: undefined;

        var script = document.createElement('script');
        script.defer = true;
        script.src = webPushConfiguration.bundleSource;
        script.onload = onLoad;

        document.body.appendChild(script);
    });
</script>

